.flex {
    flex-direction: row;
}

.flex .item {
    flex: 1
}

.service_selection {
    padding: 15px 10px;
    background: #fff;
}

.service_selection label {
    background: #cf238a;
    padding: 6px 7px;
    font-size: 20px;
    border-radius: 4px;
    display: flex;
    justify-content: center;
}

.service_selection .is_checked {
    border: 1px solid #fe002a;
    color: #00fe4c;
    background: #fff;
}

.service_selection .is_picked {
    border: 1px solid #fe0002;
    color: #fe0002;
    background: #fff;
}

.btn_switchob {
