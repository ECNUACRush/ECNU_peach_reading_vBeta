.intro_list {
    width: 105%;
    height: 80rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
}

.tag {
    background-color: #F6f7f6;
    color: #A2A2A2;
    font-size: 26rpx;
    margin: auto;
    height: 60rpx;
}

.r_tag {
    background-color: #07c160;
    color: #ffffff;
    font-size: 24rpx;
    margin-left: 100rpx;
    height: 60rpx;
}

.divide {
    border-width: 1px;
    border-color: gainsboro;
    border-style: solid;
}

.pay-hint-my-content {
    margin-left: 5%;
    margin-right: 0%;
    margin-top: 10rpx;
    font-size: 26rpx;
    color: #888;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    text-overflow: ellipsis;
    overflow: hidden;
}

.pay-hint-content {
    margin-left: 5%;
    margin-right: 5%;
    margin-top: 10px;
    font-size: 13px;
    color: #888;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    text-overflow: ellipsis;
    overflow: hidden;
}

.ellipsis {
    -webkit-line-clamp: 1;
    opacity: 0.75
}

.unellipsis {
    -webkit-line-clamp: 0;
    opacity: 1
}

.bicon {
